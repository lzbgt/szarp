	<define name="params">
		<element name="params" ns="http://www.praterm.com.pl/SZARP/ipk">
			<ref name="attlist.params"/>
			<interleave>
				<oneOrMore>
					<ref name="device"/>
				</oneOrMore>
				<optional>
					<ref name="defined"/>
				</optional>
				<optional>
					<ref name="drawdefinable"/>
				</optional>
				<optional>
					<ref name="seasons"/>
				</optional>
				<optional>
					<ref name="checker"/>
				</optional>
			</interleave>
		</element>
	</define>
	<define name="attlist.params" combine="interleave">
		<attribute name="version">
			<value>1.0</value>
		</attribute>
		<attribute name="read_freq">
			<data type="integer"/>
		</attribute>
		<attribute name="send_freq">
			<data type="integer"/>
		</attribute>
		<attribute name="title">
			<text/>
		</attribute>
		<optional>
			<attribute name="documentation_base_url"/>
		</optional>
		<optional>
			<attribute name="ps_address"/>
		</optional>
		<optional>
			<attribute name="ps_port"/>
		</optional>
		<!-- ipk-icinga -->
		<optional>
			<attribute name="host_template" ns="http://www.praterm.com.pl/SZARP/ipk-icinga"/>
		</optional>
		<optional>
			<attribute name="parent" ns="http://www.praterm.com.pl/SZARP/ipk-icinga"/>
		</optional>
		<optional>
			<attribute name="address" ns="http://www.praterm.com.pl/SZARP/ipk-icinga"/>
		</optional>
	</define>

  <!--
