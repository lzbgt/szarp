			<ref name="attlist.device"/>
			<choice>
				<oneOrMore>
					<ref name="radio"/>
				</oneOrMore>
				<oneOrMore>
					<ref name="unit"/>
				</oneOrMore>
			</choice>
		</element>
	</define>
